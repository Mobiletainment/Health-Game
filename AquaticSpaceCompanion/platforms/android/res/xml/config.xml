<?xml version='1.0' encoding='utf-8'?>
<widget id="net.pertiller.aquaspacecompanion" version="0.49" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="App">
        <param name="android-package" value="org.apache.cordova.App" />
    </feature>
    <feature name="TestFlightSDK">
        <param name="android-package" value="jp.wizcorp.phonegap.plugin.testflight.TestFlightPlugin" />
    </feature>
    <name>AquaticSpaceCompanion</name>
    <description>The Companion App to the Aquatic Space Health Game</description>
    <author email="david.pertiller@gmail.com" href="http://www.pertiller.net">David Pertiller</author>
    <access origin="*" />
    <preference name="fullscreen" value="true" />
    <preference name="StatusBarOverlaysWebView" value="false" />
    <preference name="disallowOverscroll" value="true" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="webviewbounce" value="false" />
</widget>
